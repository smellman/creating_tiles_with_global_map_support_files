.lu {
  raster-opacity:1;
  raster-scaling:near;
  raster-colorizer-default-mode: exact;
  raster-colorizer-default-color: transparent;
  raster-colorizer-stops:
    stop(10, #4ba54b)
    stop(20, #6bad88)
    stop(30, #a3fd4b)
    stop(40, #f1ff92)
    stop(50, #93dcc3)
    stop(60, #c9c9c9)
    stop(70, #ff94da)
    stop(80, #99ffff)
    stop(90, #284eb0)
}

.lc {
  raster-opacity:1;
  raster-scaling:near;
  raster-colorizer-default-mode: exact;
  raster-colorizer-default-color: transparent;
  raster-colorizer-stops:
    stop(1, #4b924b)
    stop(2, #4ba54b)
    stop(3, #87976c)
    stop(4, #63ad63)
    stop(5, #6bad88)
    stop(6, #66ca66)
    stop(7, #6edc6e)
    stop(8, #75c99b)
    stop(9, #b8dc6e)
    stop(10, #a3fd4b)
    stop(11, #93dcc3)
    stop(12, #f1ff92)
    stop(13, #ff94da)
    stop(14, #d5ee97)
    stop(15, #ffffff)
    stop(16, #c9c9c9)
    stop(17, #284eb0)
    stop(255, transparent)
}